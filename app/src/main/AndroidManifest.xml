<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ok.pinjamlaptop">

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="23"
        android:maxSdkVersion="26"/>


    <!--android:theme="@style/Theme.AppCompat.Light"-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name="BottomNavigation.GlobalVariable"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="LoginRegister.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="LoginRegister.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="BottomNavigation.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="BottomNavigation.AddMerek"
            android:screenOrientation="portrait" />
        <activity
            android:name="BottomNavigation.AddProcessor"
            android:screenOrientation="portrait" />
        <activity
            android:name="BottomNavigation.AddVGA"
            android:screenOrientation="portrait" />
        <activity
            android:name="BottomNavigation.AddAnggota"
            android:screenOrientation="portrait" />
        <activity
            android:name="BottomNavigation.AddLaptop"
            android:screenOrientation="portrait" />
        <activity
            android:name="BottomNavigation.AddPeminjaman"
            android:screenOrientation="portrait" />
        <activity
            android:name="BottomNavigation.AddPengembalian"
            android:screenOrientation="portrait" />
    </application>

</manifest>